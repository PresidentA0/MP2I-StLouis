#include <time.h>
#include <stdlib.h>



int notTrivial (int n)
{
    int a = 0 ;
    int s = 1 ;
    int t = 1 ;
    while (s <= n)
    {
        a++ ;
        s = s + t + 2 ;
        t = t + 2 ;
    }
    int inv1 = t/2+1 ;
    int inv2 = t+1 ;
    int inv3 = inv2/2 ;
    if (s != inv1 * inv1)
        printf("Il est passe par ici \n") ;
    if (s != inv2 * inv2 /4)
        printf("Il repassera par la \n") ;
    if (s != inv3 * inv3)
        printf("Est-il ici aussi ? \n") ;
    return a ;
}

int main ()
{
  // Initialization, should only be called once.
  srand(time(NULL));   
  // int r = rand() ;
  int r = 1000000 ;
  printf("Je commence ... \n") ;
  for (int  i = 0; 2*i < r; i++)
    notTrivial(i) ;
  printf("... J'en suis a la moitie et y a rien ...\n") ;
  for (int i = r/2; i < r; i++)
    notTrivial(i) ;
  printf("... J'ai fini et toujours rien.") ;
}
